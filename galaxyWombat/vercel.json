{
    "version": 2,
    "builds": [
        {
            "src": "src/main.tsx",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build" 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
